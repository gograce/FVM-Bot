{
    "version": 2,
    "buildCommand": "npm run build",
    "devCommand": "npm run start:dev",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "dist/app.js",
            "use": "@vercel/node"
        }
      ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/app.js"
        }
    ]
}